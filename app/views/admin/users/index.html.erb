    <div class="col-md-8 offset-md-1">
  		<h2>Users</h2>
  		<table class="table">
  <thead align="center">
    <tr>
      <th>image</th>
      <th>name</th>

      <th colspan="3"></th>
    </tr>

  <% @users.each do |user| %>

   <tr>
    <td>
         
         <%= image_tag user.get_profile_image(60,60), class:"mt-3 rounded-circle"; %>
         
         <br>
          </td>
     <td><%= user.name %> </td>

      <td><%= link_to "Show",admin_user_path(user.id) %></td>

  <% end %>
    </tr>
  </tbody>
</table>
<div class="row">
 <div class="col-12 d-flex justify-content-center mt-5 ">
   <%= paginate @users %>
 </div>
</div>